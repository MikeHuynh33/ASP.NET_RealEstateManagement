<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/index.css")
</head>
<body>
    <div class="navbar">
            <div class="navbar-header">
                @Html.ActionLink("Real Estate Management", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <ul class="navbar_Links"
                <li>
                    @Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "navbar_link" })
                </li>
                <li>
                    @Html.ActionLink("About Us", "Index", "Home", new { area = "" }, new { @class = "navbar_link" })
                </li>
                <li>
                    @Html.ActionLink("Contact Us", "Index", "Home", new { area = "" }, new { @class = "navbar_link" })
                </li>
            </ul>
            <div class="navbar-Login">
                @Html.Partial("_LoginPartial", new {@class="navbar-username"})
            </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <footer>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
